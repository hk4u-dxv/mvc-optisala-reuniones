# Configuración de la Base de Datos PostgreSQL
# Reemplaza estos valores con tu configuración real y guarda como .env

# Servidor de la base de datos
DB_HOST=localhost

# Nombre de la base de datos
DB_NAME=nombre_base_datos

# Usuario de PostgreSQL
DB_USER=usuario_postgres

# Contraseña (usa una contraseña segura en producción)
DB_PASSWORD=tu_contraseña

# Puerto de PostgreSQL (opcional, el puerto por defecto es 5432)
DB_PORT=5432
